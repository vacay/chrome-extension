@import '00_variables.styl'
@import 'nib'

.title
.sub-title
    display inline-block
    position relative

.title
    font-weight 400

.sub-title
    font-size .8em
    color $text-light
    text-transform uppercase

.title + .sub-title
.sub-title + .sub-title
.title > .sub-title
    margin-left 5px